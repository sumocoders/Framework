{
    "name": "sumocoders/framework",
    "license": "MIT",
    "type": "project",
    "description": "The Sumocoders framework based on Symfony",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",

        "symfony/symfony": "^4.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/migrations": "^1.4",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "twig/extensions": "^1.3",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/monolog-bundle": "^3.0",
        "sensio/framework-extra-bundle": "^3.0",
        "mopa/bootstrap-bundle": "^3.0",
        "jms/i18n-routing-bundle": "^3.0",
        "knplabs/knp-menu-bundle": "^2.1",
        "tijsverkoyen/css-to-inline-styles": "^2.0",
        "sumocoders/framework-search-bundle": "dev-upgrade-symfony-4",
        "sumocoders/framework-example-bundle": "dev-upgrade-symfony-4",
        "sumocoders/framework-multi-user-bundle": "dev-upgrade-symfony-4",
        "simple-bus/symfony-bridge": "^5.1",
        "simple-bus/doctrine-orm-bridge": "^5.0",
        "gedmo/doctrine-extensions": "^2.4",
        "friendsofsymfony/jsrouting-bundle": "^2.0",
        "sentry/sentry-symfony": "^2.0",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "fzaninotto/faker": "^1.5",
        "symfony/phpunit-bridge": "^4.0",
        "phpunit/phpunit": "^5.2 || ^4.8",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "scripts": {
        "post-install-cmd": [
            "SumoCoders\\FrameworkCoreBundle\\Composer\\ScriptHandler::createInitialConfig",
            "SumoCoders\\FrameworkCoreBundle\\Composer\\ScriptHandler::installNPM",
            "SumoCoders\\FrameworkCoreBundle\\Composer\\ScriptHandler::gulpBuild"
        ],
        "post-update-cmd": [
            "SumoCoders\\FrameworkCoreBundle\\Composer\\ScriptHandler::installNPM",
            "SumoCoders\\FrameworkCoreBundle\\Composer\\ScriptHandler::gulpBuild"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sumocoders/FrameworkMultiUserBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sumocoders/FrameworkSearchBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sumocoders/FrameworkExampleBundle"
        }
    ]
}
